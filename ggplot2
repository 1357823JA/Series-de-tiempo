version
net<-read.csv("C:\\Users\\ALUMNO\\Documents\\Historico NFLX.csv")
netts<- ts(net, start = 2018, frequency = 272)
plot(netts)
#### metodo media
plot(netts)
meanf(st,h)
media<-meanf(netts, 20)
plot(media)
##m.ingenuo
naive(st, h)
ingenuo<- naive(netts,20)
plot(ingenuo)
ingenuo
##ingenuo estacional
snaive(st,h)
ingestacional<-snaive(netts,272)
plot(ingestacional)
### metodo de la derivada
rwf(y,h,drift = TRUE)
deri<-rwf(netts,20,drift = TRUE)
plot(deri)
####
cerveza2<- window(ausbeer,start=1992,end=c(2007,4))
cervesa2
#grafica algunos pronosticos
autoplot(cerveza2)+
autolayer(naive(cerveza2, h=11),series"media", PI= FALSE)+
autolayer(snaive(cerveza2, h=11),series"ingenuo", PI= FALSE)+ 
autolayer(rwf(cerveza2, h=11,drift = T),series="ingenuo estacional", PI= FALSE)+
ggtitle("pronosticos produccion cerveza")+ xlab("año")+
ylab("miles de litros")+ guides(colour=guide_legend(title="pronostico"))
help("ausbeer")
### grafica2
help()
rwf(y,h, drift = TRUE)
deri<- rwf(netts,20, drift = TRUE)
plot(deri)
cerveza2<- window(ausbeer,start=1992,end=c(2007,4))
##Grafica algunos pronosticos
autoplot(cerveza2)+autolayer(meanf(cerveza2, h=11), series="media", PI=FALSE)+
autolayer(naive(cerveza2,h=11), series="ingenuo",
  PI=FALSE)+ autolayer(snaive(cerveza2,h=11), series = "ingenuo estacional",PI=FALSE)+
autolayer(rwf(cerveza2,h=11, drift=T),series = "deriva", PI=FALSE)+
ggtitle("pronosticos produccion cerveca")+xlab("año")+
ylab("miles de litros")+guides(colour=guide_legend(title="pronostico"))
help("ausbeer")
